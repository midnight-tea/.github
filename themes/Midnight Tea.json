	"semanticHighlighting": true,
	"semanticTokenColors": {
		"enumMember": {
			"foreground": "#E95678"
		}
	},
	"tokenColors": [
		{
			"description": "Responsible for Quote color",
			"name": "Quotes",
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#FAB795"
			}
		},
		{
			"description": "Responsible for commas",
			"name": "Commas",
			"scope": "punctuation.separator.delimiter",
			"settings": {
				"foreground": "#a9afb3"
			}
		},
		{
			"description": "Responsible for (curly) brackets / blocks",
			"name": "C++ block",
			"scope": [
				"punctuation.section.block.begin.bracket.curly.cpp",
				"punctuation.section.block.end.bracket.curly.cpp"
			],
			"settings": {
				"foreground": "#a9afb3"
			}
		},
		{
			"description": "Responsible for const variables names, for example `const auto& **item**`",
			"name": "Constant Names",
			"scope": "variable.other.constant",
			"settings": {
				"foreground": "#E95678"
			}
		},
		{
			"description": "Responsible for non const variables names, for example `std::set<std::string> **set**`",
			"name": "Non Const Names",
			"scope": "variable.other.readwrite",
			"settings": {
				"foreground": "#E95678"
			}
		},
		{
			"description": "Responsible for variable names (function parameters, class members)",
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"foreground": "#E95678"
			}
		},
		{
			"description": "Responsible for control keywords, for example `return`, `if`, `using`, `#ifdef`",
			"name": "Control Keywords",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.delete",
				"keyword.operator.expression"
			],
			"settings": {
				"foreground": "#B877DB"
			}
		},
		{
			"description": "Responsible for operators",
			"name": "Operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#a9afb3"
			}
		},
		{
			"description": "Responsible for namespace names",
			"name": "Namespaces",
			"scope": "entity.name.namespace",
			"settings": {
				"foreground": "#FAB795"
			}
		},
		{
			"description": "Responsible for function names",
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#25B0BC"
			}
		},
		{
			"description": "Responsible for types, for example `int`, `auto`, also class names",
			"name": "Types / Classes",
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#FAB795"
			}
		},
		{
			"description": "Responsible for primitive return types, private, public, modifiers",
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#B877DB"
			}
		},
		{
			"description": "Responsible for strings (what's inside of the quotes), also for includes in angled brackets",
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#FAB795"
			}
		},
		{
			"description": "Responsible for numbers",
			"name": "Integers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#F09483"
			}
		},
		{
			"description": "Responsible for constants, for example `nullptr`",
			"name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#FAB795"
			}
		},
		{
			"description": "Responsible for comments",
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#606060"
			}
		},
		{
			"description": "Responsible for: e.g. #pragma **once**",
			"name": "Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#FAB795"
			}
		},
		{
			"description": "Responsible for things like [[maybe_unused]]",
			"name": "Attributes",
			"scope": "support.other.attribute.cpp",
			"settings": {
				"foreground": "#FAB795"
			}
		}
	],
	"type": "dark"
}